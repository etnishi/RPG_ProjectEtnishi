function create_Item(){
	var iId = 0
	var nam = "Test"
	var desc = "Placehold Description"
	var func = ""
	
	var item = []
	item[0] = iId		// ID
	item[1] = nam		// Name
	item[2] = desc		// Description
	item[3] = func		// Use Function
	
	return item
}